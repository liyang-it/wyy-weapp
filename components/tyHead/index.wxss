/* 导航栏css开始*/
.left-capsule{
  /*100vm 相当于 屏幕多大 就多大*/
  width: 100%;
  /* height: 100vh; */
  
}
.left-capsule .left-capsule-nav{
  width: 100%;
  position: fixed;
  z-index: 9999;
  
}
.left-capsule-nav .left-capsule-nav-content{
  width: 85px;
  text-align: center;
  border-radius: 50px;
  position: relative;
  top: 26px;
  left: 20px;
  box-shadow:0px 0px 1px 0.2px white;
  background-color: #1d19195c;
  height: 30px;
}
.left-capsule-nav-content view{
  display: inline;
  width: 35px;
  position: relative;
}
.left-capsule-nav-content .back{
  top: 4px;left: -5px;
}
.left-capsule-nav-content .line{
    top: 3px;
    width: 1px;
    border-left: solid #cac3c3 thin;
    height: 17px;
    display: inline-block;
}
.left-capsule-nav-content .home{
  top: 4px;
}
.left-capsule-nav-title{
  color: white;
  text-align: center;
  position: absolute;
  margin: 0 auto;
  left: 0;
  right: 0;
  width: 45%;
}